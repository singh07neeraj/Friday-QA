{
  "extends": [
    "@central-products",
    "@central-products/eslint-config/overrides/node-shared-globals",
    "@central-products/eslint-config/overrides/browser",
    "@central-products/eslint-config/overrides/module",
    "@central-products/eslint-config/overrides/prettier"
  ],
  "env": {
    "mocha": true
  },
  "globals": {
    "$": true,
    "$$": true,
    "browser": true,
    "expect": true
  },
  "rules": {
    "camelcase": ["error", { "allow": ["bs_local", "real_mobile", "os_version"] }],
    "no-console": "off"
  }
}
